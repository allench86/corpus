  <%= form_tag search_search_path, :method => 'get' do %>
    <div class="search search_form">
      <%= label_tag(:search, "Search for:") %>
      <%= text_field_tag :search, params[:search], class: "keyword" %>
      <%= submit_tag "Go", name: nil, class: "btn btn-large btn-primary" %>
    </div>
  <% end %>